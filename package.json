{
  "name": "aviasalesapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "cra-template": "1.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-scripts": "5.0.1",
    "sass": "^1.83.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint */**/*.{js,jsx,html}",
    "lint:fix": "eslint --fix */**/*.{js,jsx,html}",
    "format": "prettier . --write",
    "precommit": "lint-staged",
    "prepare": "husky"
  },
  "lint-staged": {
    ".": [
